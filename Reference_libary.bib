	keywords = {4855 Phytoplankton, 3030 Micropaleontology, Fragilariopsis kerguelensis, 4912 Biogeochemical cycles, processes, and modeling, 0439 Ecosystems, structure and dynamics, morphometry, iron fertilization, 4944 Micropaleontology, Southern Ocean},
	pages = {PA1208},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/QZ77UA3A/Cortese et al. - 2012 - Glacial-interglacial size variability in the diato.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/GJTBJJCW/abstract.html:text/html}
