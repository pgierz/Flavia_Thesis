	keywords = {Signal Transduction, SH, Enzyme kinetics, Enzymes, Circadian Rhythm: physiology, Circadian clock, Circadian Rhythm, enzyme activity, Mathematics, circadian outputs, metabolic pathways, metabolic rhythm, glucose metabolism, Metabolic regulation, metabolic response analysis, Enzyme expression, metabolism},
