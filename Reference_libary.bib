	keywords = {clock, Hormones, cycle, SH, activity, locomotor activity, mating behaviour, Clock gene, Cryptochrome, clk, cyc, cry, Insects, diapause, bHLH-PAS, Methoprene-tolerant, Mating behavior, photoperiodism},
