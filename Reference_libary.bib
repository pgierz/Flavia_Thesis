	keywords = {Cyclopoid copepods, SH, predation impact, latitude, predator avoidance, DVM, field studies, predator-induced vertical migration, Cladocera, Copepods, freshwater, latitudinal gradient, diel rhythm, predation pressure, Europe, mesocosm, predatory fishes, zooplankton, photoperiodism},
	pages = {585--591},
