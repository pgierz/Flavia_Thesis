	keywords = {Algae, Entrainment, SH, Light, Circadian clock, Phytoplankton, robustness, Ostreococcus, photoperiodism},
	pages = {781--790},
	file = {Pfeuty et al. - 2012 - Circadian clocks in changing weather and seasons .pdf:/Users/fhoering/Zotero/storage/H5UMQGW9/Pfeuty et al. - 2012 - Circadian clocks in changing weather and seasons .pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/TNVBG66F/abstract.html:text/html}
}

@article{richardson_physiological_1996,
	title = {Physiological and {Optical} {Properties} of \textit{{Rhizosolenia} {Formosa}} (bacillariophyceae) in the {Context} of {Open}-{Ocean} {Vertical} {Migration}},
	volume = {32},
	issn = {1529-8817},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.0022-3646.1996.00741.x/abstract},
	doi = {10.1111/j.0022-3646.1996.00741.x},
	abstract = {Cultures of Rhizosolenia formosa H. Peragallo were studied to assess whether or not physiological and optical characteristics of this large diatom were consistent with the ability to migrate vertically in the open ocean. Time-course experiments examined changes in chemical composition and buoyancy of R. formosa during nitrate (N)–replete growth, N starvation, and recovery. Cells could maintain unbalanced growth for at least 53 h after depletion of ambient nitrate. Increases in C:N and carbohydrate: protein ratios observed during N starvation reversed within 24 h of reintroduction of nitrate to culture medium. Buoyancy was related to nutrition: Upon N depletion, the percentage of positively buoyant cells decreased to 4\% from 11\% but reverted to 9\% within 12 h of nitrate readdition. Cells took up nitrate in the dark. Nitrogen-specific uptake rates averaged 0.48 d−1; these rates were higher than N-specific growth rates (0. 15 d−1), indicating the potential for luxury consumption of nitrate, which can be stored for later use. Measurements of photosynthesis vs. irradiance, chlorophyll-specific absorption (aph*(λ)), and pigment composition showed that cells may be adapted for growth under a wide range of irradiances. Values of aph*(λ) were lower for N-depleted cells than for N-replete cells, and N-depleted cells had higher ratios of total carotenoids to chlorophyll a. Aggregation of chloroplasts was more pronounced in N-depleted cells. These are possibly photoprotective mechanisms that would be an advantage to N-depleted cells in surface waters. Compounds that absorb in the ultraviolet region were detected in N-replete cells but were absent in N-depleted cultures. Overall, these results have important implications for migrations of Rhizosolenia in nature. Cells may survive fairly long periods in N-depleted surface waters and will continue to take up carbon; then they can resume nitrate uptake and revert to positive buoyancy upon returning to deep, N-rich water. Uncoupled uptake of carbon and nitrogen during migrations of Rhizosolenia is a form of new production that may result in the net removal of carbon from oceanic surface waters.},
	language = {en},
	number = {5},
	urldate = {2017-04-03},
	journal = {Journal of Phycology},
	author = {Richardson, Tammi L. and Ciotti, Áurea M. and Cullen, John J. and Villareal, Tracy A.},
	month = oct,
	year = {1996},
	keywords = {Algae, Chlorophyll a, SH, Protein, DVM, buoyancy, Phytoplankton, diatoms, Carbohydrates},
	pages = {741--757},
	file = {Richardson et al. - 1996 - Physiological and Optical Properties of Rhizosolen.pdf:/Users/fhoering/Zotero/storage/SE9AMJ2Q/Richardson et al. - 1996 - Physiological and Optical Properties of Rhizosolen.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/KS7PR36P/abstract.html:text/html}
}

@article{li_functional_2010,
	title = {Functional {Clustering} of {Periodic} {Transcriptional} {Profiles} through {ARMA}(p,q)},
