	month = oct,
	year = {2004},
	pmid = {15514158},
	keywords = {Brain, Photoreception, In situ hybridization, SH, Opsins, Polychaete, Phylogeny, Neuroscience, Platynereis, c-opsin, r-opsin},
	pages = {869--871},
