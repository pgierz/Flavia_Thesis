@article{weydmann_pan-arctic_2016,
	title = {Pan-{Arctic} population of the keystone copepod {Calanus} glacialis},
	issn = {0722-4060, 1432-2056},
	url = {http://link.springer.com/article/10.1007/s00300-016-1898-x},
	doi = {10.1007/s00300-016-1898-x},
	abstract = {The copepod Calanus glacialis is endemic to the Arctic Ocean and peripheral seas and forms a key component of the Arctic marine ecosystems. It is the major contributor to zooplankton biomass, a predominant grazer, and an important prey for seabirds, and fish. As for a planktonic species, its dispersal is expected to be widespread and mediated by ocean currents. However, complex circulation patterns and the existence of semi-enclosed fjords and seas in the Arctic can be hypothesized to influence the population genetic structure of this species. In this study, we aimed to infer patterns of connectivity between populations of C. glacialis distributed around the Arctic and across putative barriers formed by oceanographic currents and semi-enclosed fjords and seas. To achieve this, we used 11 polymorphic microsatellite loci to genotype 189 individuals from 7 locations: Svalbard fjords (Kongsfjorden, Hornsund, Isfjorden, Rijpfjorden, and Storfjorden), White Sea, and Amundsen Gulf, thus providing greater genetic resolution over a larger biogeographical scale than in previous studies. The results revealed a lack of structure among all seven locations around the Arctic, indicating a panmictic population with large-scale gene flow. This study also supports the hypothesis that the planktonic fauna of the White Sea is not isolated from that of the other Arctic regions.},
	language = {en},
	urldate = {2016-02-26},
	journal = {Polar Biology},
	author = {Weydmann, Agata and Coelho, Nelson C. and Serrão, Ester A. and Burzyński, Artur and Pearson, Gareth A.},
	month = feb,
	year = {2016},
	keywords = {SH, Arctic, Calanus glacialis, population, distribution, Cgla, Calanoid copepods, Calanus, Copepods, Microsatellites, population genetics, Svalbard},
	pages = {1--8},
	file = {Snapshot:/Users/fhoering/Zotero/storage/23M498ZR/s00300-016-1898-x.html:text/html;Weydmann et al. - 2016 - Pan-Arctic population of the keystone copepod Cala.pdf:/Users/fhoering/Zotero/storage/7S9E3WV5/Weydmann et al. - 2016 - Pan-Arctic population of the keystone copepod Cala.pdf:application/pdf}
}

@article{w_glaciers_1998,
	title = {Glaciers caused zooplankton mortality?},
	volume = {20},
	issn = {0142-7873, 1464-3774},
	url = {http://plankt.oxfordjournals.org/content/20/7/1233},
