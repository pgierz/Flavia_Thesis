	author = {Hofmann, Eileen E. and Hüsrevolu, Yusuf Sinan},
	year = {2003},
	keywords = {Absorption efficiency, spawning, Krill, Faecal pellets, ecosystem, Scotia Sea, phytoplankton bloom, reproduction, southern ocean, model, SACCF, Fatty acids, Feeding},
	pages = {3121 -- 3142},
	annote = {(0) {\textless}ce:title{\textgreater}The \{US\} \{JGOFS\} Synthesis and Modeling Project: Phase II{\textless}/ce:title{\textgreater}},
	annote = {(166) (159) (159) (159) (171) (0) {\textless}ce:title{\textgreater}The \{US\} \{JGOFS\} Synthesis and Modeling Project: Phase II{\textless}/ce:title{\textgreater}(159) (159) (159) (160) (160) (160) (171) (159) (159) (160) (160) (159)},
