@article{tangprasittipap_structure_2012,
	title = {Structure and expression of a shrimp prohormone convertase 2},
	volume = {178},
	issn = {0016-6480},
	url = {http://www.sciencedirect.com/science/article/pii/S0016648012002018},
	doi = {10.1016/j.ygcen.2012.05.005},
	abstract = {Although many crustacean neuroendocrine hormones have been reported, the enzymes responsible for post-translational modification of neuroendocrine hormones have rarely been characterized. A prohormone convertase 2 (PC2)-like enzyme has been isolated from the optic lobe of the giant tiger shrimp, Penaeus monodon and referred as PmPC2. The full length cDNA sequence of PmPC2 has been identified and found to resemble evolutionarily conserved PC2 enzymes of vertebrates and invertebrates. PmPC2 was expressed in all larval developmental stages and in neuroendrocrine cells in the adult optic lobe. Its expression was found to be negatively related with shrimp body weight by qPCR (P{\textless}0.05). Immunohistochemistry results using an anti-rPmPC2 antibody with adult shrimp revealed high staining intensity in specific neurosecretory cells including the sinus gland, the organ of Hanström (also referred to as the medullar terminalis X-organ) and the organ of Bellonci (also referred to as the sensory or X-organ). By using the yeast two hybrid technique, PmPC2 was found to bind with P. monodon hyperglycemic hormone (Pem-CHH1) that plays an important role in glucose metabolism. Since PmPC2 is a subtilisin-like serine proteinase, it is expected to cleave the synthetic substrate, pyr-RTKR-MCA, but the expressed recombinant catalytic domain of PmPC2 (rPmPC2-cat) showed no enzymatic activity as expected. In vivo injection of dsRNA-PmPC2 resulted in reduced transcripts for both PmPC2 and Pem-CHH1 on day 3 post injection, but there was no accompanying reduction of glucose level in the hemolymph. Taken together, PmPC2 localization, expression and activity suggest that it has a function(s) in the shrimp neuroendrocrine system and that it may not only activate Pem-CHH1 but also affect its expression. However, there is no obvious explanation for the negative correlation between PmPC2 expression level and shrimp body weight.},
	number = {2},
	urldate = {2019-02-13},
	journal = {General and Comparative Endocrinology},
	author = {Tangprasittipap, Amornrat and Chouwdee, Saisunee and Phiwsaiya, Kornsunee and Laiphrom, Seansook and Senapin, Saengchan and Flegel, Timothy W. and Sritunyalucksana, Kallaya},
	month = sep,
	year = {2012},
	keywords = {Crustacean hyperglycemic hormone-1 (CHH-1), Neuroendocrine hormones, Neurosecretory cells, Prohormone convertase 2 (PC2)},
	pages = {185--193},
	file = {ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/ERI4VHVL/S0016648012002018.html:text/html;Tangprasittipap et al. - 2012 - Structure and expression of a shrimp prohormone co.pdf:/Users/fhoering/Zotero/storage/AVGFIM5N/Tangprasittipap et al. - 2012 - Structure and expression of a shrimp prohormone co.pdf:application/pdf}
}

