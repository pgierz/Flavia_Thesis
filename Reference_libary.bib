@article{kvile_disentangling_2016,
	title = {Disentangling the mechanisms behind climate effects on zooplankton},
	volume = {113},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/113/7/1841},
	doi = {10.1073/pnas.1525130113},
	abstract = {Understanding how climate influences ecosystems is complicated by the many correlated and interrelated impacting factors. Here we quantify climate effects on Calanus finmarchicus in the northeastern Norwegian Sea and southwestern Barents Sea. By combining oceanographic drift models and statistical analyses of field data from 1959 to 1993 and investigating effects across trophic levels, we are able to elucidate pathways by which climate influences zooplankton. The results show that both chlorophyll biomass in spring and C. finmarchicus biomass in summer relate positively to a combination of shallow mixed layer depth and increased wind in spring, suggesting that C. finmarchicus biomass in summer is influenced by bottom-up effects of food availability. Furthermore, spatially resolved C. finmarchicus biomass in summer is linked to favorable transport from warmer, core areas to the south. However, increased mean temperature in spring does not lead to increased C. finmarchicus biomass in summer. Rather, spring biomass is generally higher, but population growth from spring to summer is lower, after a warm compared with a cold spring. Our study illustrates how improved understanding of climate effects can be obtained when different datasets and different methods are combined in a unified approach.},
	language = {en},
	number = {7},
	urldate = {2017-03-14},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Kvile, Kristina Ø and Langangen, Øystein and Prokopchuk, Irina and Stenseth, Nils C. and Stige, Leif C.},
	month = feb,
	year = {2016},
	pmid = {26831099},
	keywords = {Cfin, SH, biomass, ecosystem, model, climate, food, Calanoid copepods, Calanus, Calanus finmarchicus, Copepods, climate change, food concentration, Time series, bottom-up control, climate effects, zooplankton},
	pages = {1841--1846},
	file = {Kvile et al. - 2016 - Disentangling the mechanisms behind climate effect.pdf:/Users/fhoering/Zotero/storage/3C875W6H/Kvile et al. - 2016 - Disentangling the mechanisms behind climate effect.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/FM39RZPW/1841.html:text/html}
