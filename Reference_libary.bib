	year = {2011},
	keywords = {Boreogadus saida, polar night, fish, SH, copepod prey, mesozooplankton, Arctic, high latitude, DVM, polar cod, zooplankton, Sea Ice, photoperiodism},
	pages = {1959--1971},
	file = {Geoffroy et al. - 2011 - The aggregation of polar cod (Boreogadus saida) in.pdf:/Users/fhoering/Zotero/storage/6HTCJZRZ/Geoffroy et al. - 2011 - The aggregation of polar cod (Boreogadus saida) in.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/QB8R9T2N/s00300-011-1019-9.html:text/html}
