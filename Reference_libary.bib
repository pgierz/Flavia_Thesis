	month = dec,
	year = {2017},
	pmid = {29263132},
	keywords = {SH, Arctic, Calanus hyperboreus, Calanus glacialis, calanus helgolandicus, north Atlantic, Calanoid copepods, Calanus, Calanus finmarchicus, Copepods, population genetics},
