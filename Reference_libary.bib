	journal = {Nucleic Acids Research},
	author = {Larriba, Yolanda and Rueda, Cristina and Fernández, Miguel A. and Peddada, Shyamal D.},
	month = dec,
	year = {2016},
	pmid = {27596593},
	pmcid = {PMC5159537},
	pages = {e163},
	file = {PubMed Central Full Text PDF:/Users/fhoering/Zotero/storage/XWNKH3VT/Larriba et al. - 2016 - Order restricted inference for oscillatory systems.pdf:application/pdf}
}

