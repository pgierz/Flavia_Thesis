@article{roenneberg_circadian_2005,
	title = {Circadian clocks — the fall and rise of physiology},
	volume = {6},
	copyright = {© 2005 Nature Publishing Group},
	issn = {1471-0072},
	url = {http://www.nature.com/nrm/journal/v6/n12/abs/nrm1766.html},
	doi = {10.1038/nrm1766},
	abstract = {Circadian clocks control the daily life of most light-sensitive organisms — from cyanobacteria to humans. Molecular processes generate cellular rhythmicity, and cellular clocks in animals coordinate rhythms through interaction (known as coupling). This hierarchy of clocks generates a complex, 24-hour temporal programme that is synchronized with the rotation of the Earth. The circadian system ensures anticipation and adaptation to daily environmental changes, and functions on different levels — from gene expression to behaviour. Circadian research is a remarkable example of interdisciplinarity, unravelling the complex mechanisms that underlie a ubiquitous biological programme. Insights from this research will help to optimize medical diagnostics and therapy, as well as adjust social and biological timing on the individual level.},
	language = {en},
	number = {12},
