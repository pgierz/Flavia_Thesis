@article{thommen_robustness_2010,
	title = {Robustness of {Circadian} {Clocks} to {Daylight} {Fluctuations}: {Hints} from the {Picoeucaryote} \textit{{Ostreococcus} tauri}},
	volume = {6},
	issn = {1553-7358},
	shorttitle = {Robustness of {Circadian} {Clocks} to {Daylight} {Fluctuations}},
	url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1000990},
	doi = {10.1371/journal.pcbi.1000990},
	abstract = {Author Summary Circadian clocks keep time of day in many living organisms, allowing them to anticipate environmental changes induced by day/night alternation. They consist of networks of genes and proteins interacting so as to generate biochemical oscillations with a period close to 24 hours. Circadian clocks synchronize to the day/night cycle through the year principally by sensing ambient light. Depending on the weather, the perceived light intensity can display large fluctuations within the day and from day to day, potentially inducing unwanted resetting of the clock. Furthermore, marine organisms such as microalgae are subjected to dramatic changes in light intensities in the water column due to streams and wind. We showed, using mathematical modelling, that the green unicellular marine alga Ostreococcus tauri has evolved a simple but effective strategy to shield the circadian clock from daylight fluctuations by localizing coupling to the light during specific time intervals. In our model, as in experiments, coupling is invisible when the clock is in phase with the day/night cycle but resets the clock when it is out of phase. Such a clock architecture is immune to strong daylight fluctuations.},
	number = {11},
	urldate = {2017-04-03},
	journal = {PLOS Computational Biology},
	author = {Thommen, Quentin and Pfeuty, Benjamin and Morant, Pierre-Emmanuel and Corellou, Florence and Bouget, François-Yves and Lefranc, Marc},
	month = nov,
	year = {2010},
	keywords = {gene expression, Algae, microarray, SH, Light, Clock gene, Circadian clock, model, Circadian Rhythm, Transcription, Phytoplankton, Proteolysis, robustness, Ostreococcus},
	pages = {e1000990},
	file = {Snapshot:/Users/fhoering/Zotero/storage/P32CDIQM/article.html:text/html;Thommen et al. - 2010 - Robustness of Circadian Clocks to Daylight Fluctua.pdf:/Users/fhoering/Zotero/storage/HIW5IQ3J/Thommen et al. - 2010 - Robustness of Circadian Clocks to Daylight Fluctua.pdf:application/pdf}
}

@article{moulager_light-dependent_2007,
	title = {Light-{Dependent} {Regulation} of {Cell} {Division} in \textit{{Ostreococcus}}: {Evidence} for a {Major} {Transcriptional} {Input}},
	volume = {144},
	issn = {, 1532-2548},
