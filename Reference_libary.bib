@article{nielsen_live_2014,
	title = {Live discrimination of \textit{{Calanus} glacialis} and \textit{{C}. finmarchicus} females: can we trust phenological differences?},
	volume = {161},
	issn = {0025-3162, 1432-1793},
	shorttitle = {Live discrimination of {Calanus} glacialis and {C}. finmarchicus females},
	url = {https://link.springer.com/article/10.1007/s00227-014-2419-5},
	doi = {10.1007/s00227-014-2419-5},
	abstract = {Two key players in the Arctic and subarctic marine ecosystem are the calanoid copepods, Calanus finmarchicus and C. glacialis. Although morphologically very similar, these sibling species have different life cycles and roles in the Arctic pelagic marine ecosystem. Considering that the distribution of C. glacialis corresponds to Arctic water masses and C. finmarchicus to Atlantic water masses, the species are frequently used as climate indicators. Consequently, correct identification of the two species is essential if we want to understand climate-impacted changes on Calanus-dominated marine ecosystems such as the Arctic. Here, we present a novel morphological character (redness) to distinguish live females of C. glacialis and C. finmarchicus and compare it to morphological (prosome length) and genetic identification. The characters are tested on 300 live females of C. glacialis and C. finmarchicus from Disko Bay, western Greenland. Our analysis confirms that length cannot be used as a stand-alone criterion for separation. The results based on the new morphological character were verified genetically using a single mitochondrial marker (16S) and nuclear loci (six microsatellites and 12 InDels). The pigmentation criterion was also used on individuals (n = 89) from Young Sound fjord, northeast Greenland to determine whether the technique was viable in different geographical locations. Genetic markers based on mitochondrial and nuclear loci were corroborative in their identification of individuals and revealed no hybrids. Molecular identification confirmed that live females of the two species from Greenlandic waters, both East and West, can easily be separated by the red pigmentation of the antenna and somites of C. glacialis in contrast to the pale opaque antenna and somites of C. finmarchicus, confirming that the pigmentation criterion is valid for separation of the two species.},
	language = {en},
	number = {6},
	urldate = {2017-10-02},
	journal = {Marine Biology},
	author = {Nielsen, Torkel Gissel and Kjellerup, Sanne and Smolina, Irina and Hoarau, Galice and Lindeque, Penelope},
	month = jun,
	year = {2014},
	keywords = {Cfin, Genetic Markers, misidentification, genomics, SH, Calanus glacialis, Identification, Cgla, Female, Calanoid copepods, Calanus, Calanus finmarchicus, Copepods, phenotype},
	pages = {1299--1306},
	file = {Nielsen et al. - 2014 - Live discrimination of Calanus glacialis and C. fi.pdf:/Users/fhoering/Zotero/storage/TDK7QVF4/Nielsen et al. - 2014 - Live discrimination of Calanus glacialis and C. fi.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/IBNE4FH9/s00227-014-2419-5.html:text/html}
}

@article{schaafsma_size_2016-2,
	title = {Size and stage composition of age class 0 {Antarctic} krill ({Euphausia} superba) in the ice–water interface layer during winter/early spring},
	volume = {39},
	issn = {0722-4060, 1432-2056},
	url = {https://link.springer.com/article/10.1007/s00300-015-1877-7},
	doi = {10.1007/s00300-015-1877-7},
	abstract = {The condition and survival of Antarctic krill (Euphausia superba) strongly depends on sea ice conditions during winter. How krill utilize sea ice depends on several factors such as region and developmental stage. A comprehensive understanding of sea ice habitat use by krill, however, remains largely unknown. The aim of this study was to improve the understanding of the krill’s interaction with the sea ice habitat during winter/early spring by conducting large-scale sampling of the ice–water interface (0–2 m) and comparing the size and developmental stage composition of krill with the pelagic population (0–500 m). Results show that the population in the northern Weddell Sea consisted mainly of krill that were {\textless}1 year old (age class 0; AC0), and that it was comprised of multiple cohorts. Size per developmental stage differed spatially, indicating that the krill likely were advected from various origins. The size distribution of krill differed between the two depth strata sampled. Larval stages with a relatively small size (mean 7–8 mm) dominated the upper two metre layer of the water column, while larger larvae and AC0 juveniles (mean 14–15 mm) were proportionally more abundant in the 0- to 500-m stratum. Our results show that, as krill mature, their vertical distribution and utilization of the sea ice appear to change gradually. This could be the result of changes in physiology and/or behaviour, as, e.g., the krill’s energy demand and swimming capacity increase with size and age. The degree of sea ice association will have an effect on large-scale spatial distribution patterns of AC0 krill and on predictions of the consequences of sea ice decline on their survival over winter.},
	language = {en},
	number = {9},
	urldate = {2017-07-11},
	journal = {Polar Biology},
	author = {Schaafsma, F. L. and David, C. and Pakhomov, E. A. and Hunt, B. P. V. and Lange, B. A. and Flores, H. and van Franeker, J. A.},
	month = sep,
	year = {2016},
	pages = {1515--1526},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/7WIEC6S4/Schaafsma et al. - 2016 - Size and stage composition of age class 0 Antarcti.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/3NPKHJM8/s00300-015-1877-7.html:text/html}
}

@article{kopperud_circadian_2017-1,
	title = {Circadian {Rhythms} of {Retinomotor} {Movement} in a {Marine} {Megapredator}, the {Atlantic} {Tarpon}, \textit{{Megalops} atlanticus}},
	volume = {18},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	url = {http://www.mdpi.com/1422-0067/18/10/2068},
	doi = {10.3390/ijms18102068},
	abstract = {Many ecologically and economically important marine fish species worldwide spend portions of their lives in coastal regions that are increasingly inundated by artificial light at night. However, while extensive research illustrates the harmful effects of inappropriate light exposure on biological timing in humans, rodents and birds, comparable studies on marine fish are virtually nonexistent. This study aimed to assess the effects of light on biological clock function in the marine fish retina using the Atlantic tarpon (Megalops atlanticus) as a model. Using anti-opsin immunofluorescence, we observed robust rhythms of photoreceptor outer segment position (retinomotor movement) over the course of the daily light–dark cycle: cone outer segments were contracted toward the inner retina and rods were elongated during the day; the opposite occurred at night. Phase shifting the daily light–dark cycle caused a corresponding shift of retinomotor movement timing, and cone retinomotor movement persisted in constant darkness, indicating control by a circadian clock. Constant light abolished retinomotor movements of both photoreceptor types. Thus, abnormally-timed light exposure may disrupt normal M. atlanticus clock function and harm vision, which in turn may affect prey capture and predator avoidance. These results should help inform efforts to mitigate the effects of coastal light pollution on organisms in marine ecosystems.},
	language = {en},
	number = {10},
	urldate = {2017-10-12},
	journal = {International Journal of Molecular Sciences},
	author = {Kopperud, Kristin L. and Grace, Michael S.},
	month = sep,
	year = {2017},
	keywords = {LD/DD, spectral sensitivity, Photoreception, fish, light spectrum, laboratory experiments, Light receptors, SH, Light, endogenous rhythm, light adaptation, Circadian Rhythm, retina, Light response, retinomotor movement},
	pages = {2068},
	file = {Kopperud und Grace - 2017 - Circadian Rhythms of Retinomotor Movement in a Mar.pdf:/Users/fhoering/Zotero/storage/IPWZ7FHC/Kopperud und Grace - 2017 - Circadian Rhythms of Retinomotor Movement in a Mar.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/DV8XXNJD/htm.html:text/html}
}

@article{christie_circadian_2017,
	title = {Circadian signaling in the {Northern} krill \textit{{Meganyctiphanes} norvegica}: \textit{{In} silico} prediction of the protein components of a putative clock system using a publicly accessible transcriptome},
	issn = {1874-7787},
	shorttitle = {Circadian signaling in the {Northern} krill {Meganyctiphanes} norvegica},
	url = {http://www.sciencedirect.com/science/article/pii/S1874778717302180},
	doi = {10.1016/j.margen.2017.09.001},
	abstract = {The Northern krill Meganyctiphanes norvegica is a significant component of the zooplankton community in many regions of the North Atlantic Ocean. In the areas it inhabits, M. norvegica is of great importance ecologically, as it is both a major consumer of phytoplankton/small zooplankton and is a primary food source for higher-level consumers. One behavior of significance for both feeding and predator avoidance in Meganyctiphanes is diel vertical migration (DVM), i.e., a rising from depth at dusk and a return to depth at dawn. In this and other euphausiids, an endogenous circadian pacemaker is thought, at least in part, to control DVM. Currently, there is no information concerning the identity of the genes/proteins that comprise the M. norvegica circadian system. In fact, there is little information concerning the molecular underpinnings of circadian rhythmicity in crustaceans generally. Here, a publicly accessible transcriptome was used to identify the molecular components of a putative Meganyctiphanes circadian system. A complete set of core clock proteins was deduced from the M. norvegica transcriptome (clock, cryptochrome 2, cycle, period and timeless), as was a large suite of proteins that likely function as modulators of the core clock (e.g., doubletime), or serves as inputs to it (cryptochrome 1) or outputs from it (pigment dispersing hormone). This is the first description of a “complete” (core clock through putative output pathway signals) euphausiid clock system, and as such, provides a foundation for initiating molecular investigations of circadian signaling in M. norvegica and other krill species, including how clock systems may regulate DVM and other behaviors.},
	urldate = {2017-10-12},
	journal = {Marine Genomics},
	author = {Christie, Andrew E. and Yu, Andy and Pascual, Micah G.},
	year = {2017},
	keywords = {bioinformatics, PDP, Krill, SH, Meganyctiphanes norvegica, Clock gene, Euphausiacea, PDF/PDH},
	file = {Christie et al. - 2017 - Circadian signaling in the Northern krill iMegan.pdf:/Users/fhoering/Zotero/storage/FWQN78FS/Christie et al. - 2017 - Circadian signaling in the Northern krill iMegan.pdf:application/pdf;ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/BDX6A2EF/S1874778717302180.html:text/html}
}

