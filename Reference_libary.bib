@article{aguzzi_circadian_2003,
	title = {Circadian {Oxygen} {Consumption} {Patterns} in {Continental} {Slope} \textit{{Nephrops} norvegicus} ({Decapoda}: {Nephropidae}) in the {Western} {Mediterranean}},
