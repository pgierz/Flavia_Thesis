	pages = {58--73},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/5MRUFVST/Roth et al. - 2007 - Evolution after gene duplication models, mechanis.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/VW3956RC/abstract.html:text/html}
