	month = aug,
	year = {2017},
	keywords = {Algae, per, timeless, tim, Molluscs, clock, laboratory experiments, SH, RNAi, Period, Clock gene, Cryptochrome, Bmal, clk, cry, bivalve, marine clocks, oyster, Crassostrea gigas, bioaccumulation, ploidy},
