	pages = {1203--1213},
	file = {NRC Research Press PDF fulltext:/Users/fhoering/Zotero/storage/LHKSKN6W/Höring et al. - 2018 - Light regime affects the seasonal cycle of Antarct.pdf:application/pdf;NRC Research Press Snapshot:/Users/fhoering/Zotero/storage/38PTF2MD/cjz-2017-0353.html:text/html}
