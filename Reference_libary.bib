	keywords = {Sea ice flux, West Spitzbergen Current, WSC, Lipid, SH, Pelagic food web, Fram Strait, Stable isotopes, Water masses, Ice biota},
	pages = {182--231},
	file = {Hop et al 2006_SH.pdf:/Users/fhoering/Zotero/storage/JGC9QQX7/Hop et al 2006_SH.pdf:application/pdf;Hop et al. - 2006 - Physical and biological characteristics of the pel.pdf:/Users/fhoering/Zotero/storage/AI5B5KWZ/Hop et al. - 2006 - Physical and biological characteristics of the pel.pdf:application/pdf;ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/FC9XTHHC/S0079661106001212.html:text/html}
