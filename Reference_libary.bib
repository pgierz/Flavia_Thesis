	month = dec,
	year = {1995},
	keywords = {Neuropeptides, Crustaceans, Chromatophorotropins, Crustacean hyperglycemic hormone, Gonad-inhibiting hormone, Molecular biology, Molt-inhibiting hormone, Pigment-dispersing hormone, Preprohormones, Red pigment concentrating hormone},
	pages = {573--579},
	file = {ScienceDirect Full Text PDF:/Users/fhoering/Zotero/storage/FM7AHU3Y/De Kleijn und Van Herp - 1995 - Molecular biology of neurohormone precursors in th.pdf:application/pdf;ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/7L2FI9P8/0305049195001263.html:text/html}
}

