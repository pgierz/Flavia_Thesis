	keywords = {SH, tidal rhythm, Circadian clock, Lunar cycle, alternative splicing, Circadian Rhythm, Insects, splicing, Ecological genetics, Evolutionary genetics, marine clocks, Clunio marinus, circalunar clock, circalunar rhythm, emergence},
