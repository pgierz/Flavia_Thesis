@article{clarke_adult_2008,
	title = {Adult antarctic krill feeding at abyssal depths.},
	volume = {18},
	issn = {0960-9822},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18302926},
	doi = {10.1016/j.cub.2008.01.059},
	abstract = {Antarctic krill (Euphausia superba) is a large euphausiid, widely distributed within the Southern Ocean [1], and a key species in the Antarctic food web [2]. The Discovery Investigations in the early 20(th) century, coupled with subsequent work with both nets and echosounders, indicated that the bulk of the population of postlarval krill is typically confined to the top 150 m of the water column [1, 3, 4]. Here, we report for the first time the existence of significant numbers of Antarctic krill feeding actively at abyssal depths in the Southern Ocean. Biological observations from the deep-water remotely operated vehicle Isis in the austral summer of 2006/07 have revealed the presence of adult krill (Euphausia superba Dana), including gravid females, at unprecedented depths in Marguerite Bay, western Antarctic Peninsula. Adult krill were found close to the seabed at all depths but were absent from fjords close inshore. At all locations where krill were detected they were seen to be actively feeding, and at many locations there were exuviae (cast molts). These observations revise significantly our understanding of the depth distribution and ecology of Antarctic krill, a central organism in the Southern Ocean ecosystem.},
	number = {4},
	journal = {Current Biology},
	author = {Clarke, Andrew and Tyler, Paul a},
	month = feb,
