@article{mrosovsky_diurnal_2005,
	title = {Diurnal mice ({Mus} musculus) and other examples of temporal niche switching},
	volume = {191},
	issn = {1432-1351},
	url = {https://doi.org/10.1007/s00359-005-0017-1},
