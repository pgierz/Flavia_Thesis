@article{pihl_production_1982,
	title = {Production, abundance, and biomass of mobile epibenthic marine fauna in shallow waters, {Western} {Sweden}},
	volume = {57},
	issn = {0022-0981},
	url = {http://www.sciencedirect.com/science/article/pii/0022098182901976},
	doi = {10.1016/0022-0981(82)90197-6},
	abstract = {Shallow coastal marine waters are generally considered highly productive and important as nursery grounds. In this study a new sampling device, a portable drop trap, is introduced. This allows quantitative sampling with high accuracy and statistically precise assessment in defined sampling areas. The production, abundance, biomass, and horizontal dispersion of the following dominant and mobile epifaunal species have been assessed over several years in two bays with a sandy-silt substratum in western Sweden: Crangon crangon (Fabricius), Carcinus maenas (L.), Pleuronectes platessa (L.), Pomatoschistus microps (Krøyer) and P. minutus (Pallas).

Recruitment of the 0-group began in April with Pleuronectes and was followed in early July by the other species. This latter immigration often occurred in waves and a separation was then made into cohorts based on lengths or widths. One year with a severe winter delayed the immigration of these species, but did not reduce the total annual production. Crangon was the dominant species and its annual maximum mean abundance was between 55 and 90 ind. ·m−2. The total mean epifaunal abundance and biomass could well exceed 50 ind. ·m−2 and 1.2 g (ash-free dry weight, AFDW) ·m−2 over several months. The total annual production in one of the bays varied over a 4-yr period between 3.8 and 5.0 g AFDW ·m−2, corresponding to 25 g fresh wt. Despite considerable intraspecific variation of abundance and production between the 0- and 1-groups of crabs and of shrimps the annual production of Crangon and Carcinus combined was between 3.5 and 4.0 g AFDW ·m−2. It is suggested that the areas studied have an intrinsic carrying capacity for these species, which partly could be regulated within the species.

The short season in Sweden is clearly reflected in that almost 90\% of the annual epifaunal production occurred during 4 months, and \&gt;50\% during August and September of each year. Nevertheless, the annual production and the mean maximum abundance of these mobile species were at least as great as the highest so far recorded in Europe.},
	number = {2–3},
	urldate = {2014-10-22},
	journal = {Journal of Experimental Marine Biology and Ecology},
	author = {Pihl, Leif and Rosenberg, Rutger},
	month = jan,
	year = {1982},
	pages = {273--301},
	file = {Pihl, Rosenberg 1982 (Production, Abundance, Biomass, Epibenthos).pdf:/Users/fhoering/Zotero/storage/F7588G9Q/Pihl, Rosenberg 1982 (Production, Abundance, Biomass, Epibenthos).pdf:application/pdf}
}

