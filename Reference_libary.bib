	keywords = {survival, Cfin, North Sea, energy content, SH, Circannual rhythm, Respiration, Scotland, seasonal cycle, Temperature, north Atlantic, Calanoid copepods, Calanus, Calanus finmarchicus, Copepods, diapause, carbon content, energy expenditure, Seasonal rhythm},
	pages = {73--83},
