	keywords = {Euphausia superba, Opsins, Antarctic krill, Nonvisual opsins, diel rhythm, Daily rhythms},
	pages = {61--68},
	file = {Biscontin et al. - 2016 - The opsin repertoire of the Antarctic krill iEup.pdf:/Users/fhoering/Zotero/storage/EJF7PGXS/Biscontin et al. - 2016 - The opsin repertoire of the Antarctic krill iEup.pdf:application/pdf;ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/XS3JW33K/S1874778716300344.html:text/html}
