	journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
	author = {Nicol, S. and Virtue, P. and King, Robert and Davenport, S. R. and McGaffin, A. F. and Nichols, P.},
	year = {2004},
	keywords = {feeding activity, gut content, gut fullness, 1st def},
	pages = {2215 -- 2224},
	file = {Nicol et al. 2004 (condition of E. cryst. in winter).pdf:/Users/fhoering/Zotero/storage/TCNUCWVS/Nicol et al. 2004 (condition of E. cryst. in winter).pdf:application/pdf}
}

@article{gachon_mammalian_2004,
	title = {The mammalian circadian timing system: from gene expression to physiology},
	volume = {113},
	issn = {0009-5915, 1432-0886},
	shorttitle = {The mammalian circadian timing system},
	url = {http://link.springer.com/article/10.1007/s00412-004-0296-2},
	doi = {10.1007/s00412-004-0296-2},
	abstract = {Many physiological processes in organisms from bacteria to man are rhythmic, and some of these are controlled by self-sustained oscillators that persist in the absence of external time cues. Circadian clocks are perhaps the best characterized biological oscillators and they exist in virtually all light-sensitive organisms. In mammals, they influence nearly all aspects of physiology and behavior, including sleep-wake cycles, cardiovascular activity, endocrinology, body temperature, renal activity, physiology of the gastro-intestinal tract, and hepatic metabolism. The master pacemaker is located in the suprachiasmatic nuclei, two small groups of neurons in the ventral part of the hypothalamus. However, most peripheral body cells contain self-sustained circadian oscillators with a molecular makeup similar to that of SCN (suprachiasmatic nucleus) neurons. This organization implies that the SCN must synchronize countless subsidiary oscillators in peripheral tissues, in order to coordinate cyclic physiology. In this review, we will discuss some recent studies on the structure and putative functions of the mammalian circadian timing system, but we will also point out some apparent inconsistencies in the currently publicized model for rhythm generation.},
	number = {3},
	urldate = {2014-07-22},
	author = {Gachon, Frédéric and Nagoshi, Emi and Brown, Steven A. and Ripperger, Juergen and Schibler, Ueli},
	month = sep,
	year = {2004},
	pages = {103--112},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/4W9HDTI5/Gachon et al. - 2004 - The mammalian circadian timing system from gene e.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/UJNDAQX2/s00412-004-0296-2.html:text/html}
}

