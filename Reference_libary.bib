@article{pecasse_kruppel-homolog_2000,
	title = {Krüppel-homolog, a {Stage}-{Specific} {Modulator} of the {Prepupal} {Ecdysone} {Response}, {Is} {Essential} for {Drosophila} {Metamorphosis}},
