	pmid = {27773695},
	keywords = {gene expression, per, Mammals, clock, Entrainment, mouse, Period, Clock gene, Cryptochrome, clk, Circadian clock, cry, Circadian Rhythm, oxygen, HIF, Jetlag, metabolism},
	pages = {93--101},
