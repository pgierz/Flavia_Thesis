@article{rozema_interannual_2016,
	title = {Interannual variability in phytoplankton biomass and species composition in northern {Marguerite} {Bay} ({West} {Antarctic} {Peninsula}) is governed by both winter sea ice cover and summer stratification},
	issn = {1939-5590},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/lno.10391/abstract},
	doi = {10.1002/lno.10391},
	abstract = {The rapid warming of the West Antarctic Peninsula region has led to reduced sea ice cover and enhanced glacial melt water input. This has potential implications for marine ecosystems, notably phytoplankton growth, biomass, and composition. Fifteen years (1997–2012) of year-round size fractionated chlorophyll a (Chl a), phytoplankton pigment fingerprinting and environmental data were analyzed to identify the relationship between sea ice cover, water column stability and phytoplankton dynamics in northern Marguerite Bay, Antarctica. Over the investigated period, both summer (December–February) and winter biomass declined significantly, 38.5\% and 33.3\% respectively. Winter phytoplankton biomass was low ({\textless} 0.25 μg Chl a L−1) and consisted on average of 69\% diatoms, 5\% cryptophytes, and 20\% haptophytes. Summers following winters with low ({\textless} 65 days) sea ice cover were characterized by decreased stratification strength and relatively low (median {\textless} 4.4 μg Chl a L−1) phytoplankton biomass, as compared to summers preceded by high winter sea ice cover. In addition, the summertime microphytoplankton ({\textgreater} 20 μm) fraction was strongly decreased in the low biomass years, from 92\% to 39\%, coinciding with a smaller diatom fraction in favor of nanophytoplankton ({\textless} 20 μm), represented by cryptophytes and haptophytes. In contrast, diatoms dominated ({\textgreater} 95\%) during summers with average-to-high biomass. We advance a conceptual model whereby low winter sea ice cover leads to low phytoplankton biomass and enhanced proportions of nanophytoplankton, when this coincides with reduced stratification during summer. These changes are likely to have a strong effect on the entire Antarctic marine food web, including krill biomass, and distribution.},
	language = {en},
	urldate = {2016-10-19},
	journal = {Limnology and Oceanography},
	author = {Rozema, P. D. and Venables, H. J. and van de Poll, W. H. and Clarke, A. and Meredith, M. P. and Buma, A. G. J.},
	month = oct,
	year = {2016},
	pages = {n/a--n/a},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/JVMGKDXF/Rozema et al. - 2016 - Interannual variability in phytoplankton biomass a.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/FFNWCBHT/abstract.html:text/html}
}

