@article{tataroglu_calcium_2015,
	title = {Calcium and {SOL} {Protease} {Mediate} {Temperature} {Resetting} of {Circadian} {Clocks}},
	volume = {163},
	issn = {0092-8674},
	url = {http://www.sciencedirect.com/science/article/pii/S0092867415013458},
	doi = {10.1016/j.cell.2015.10.031},
	abstract = {Summary
Circadian clocks integrate light and temperature input to remain synchronized with the day/night cycle. Although light input to the clock is well studied, the molecular mechanisms by which circadian clocks respond to temperature remain poorly understood. We found that temperature phase shifts Drosophila circadian clocks through degradation of the pacemaker protein TIM. This degradation is mechanistically distinct from photic CRY-dependent TIM degradation. Thermal TIM degradation is triggered by cytosolic calcium increase and CALMODULIN binding to TIM and is mediated by the atypical calpain protease SOL. This thermal input pathway and CRY-dependent light input thus converge on TIM, providing a molecular mechanism for the integration of circadian light and temperature inputs. Mammals use body temperature cycles to keep peripheral clocks synchronized with their brain pacemaker. Interestingly, downregulating the mammalian SOL homolog SOLH blocks thermal mPER2 degradation and phase shifts. Thus, we propose that circadian thermosensation in insects and mammals share common principles.},
	number = {5},
	urldate = {2015-11-24},
	journal = {Cell},
	author = {Tataroglu, Ozgur and Zhao, Xiaohu and Busza, Ania and Ling, Jinli and O’Neill, John S. and Emery, Patrick},
	month = nov,
	year = {2015},
	keywords = {timeless, tim, Entrainment, SH, Circadian clock, Temperature, Drosophila, clock resetting},
	pages = {1214--1224},
	file = {ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/F9NNSUW4/S0092867415013458.html:text/html;Tataroglu et al. - 2015 - Calcium and SOL Protease Mediate Temperature Reset.pdf:/Users/fhoering/Zotero/storage/72BDECXN/Tataroglu et al. - 2015 - Calcium and SOL Protease Mediate Temperature Reset.pdf:application/pdf}
}

@article{jones_changing_2015,
	title = {Changing climate cues differentially alter zooplankton dormancy dynamics across latitudes},
	copyright = {This article is protected by copyright. All rights reserved.},
	issn = {1365-2656},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/1365-2656.12474/abstract},
	doi = {10.1111/1365-2656.12474},
	abstract = {1.In seasonal climates, dormancy is a common strategy that structures biodiversity and is necessary for the persistence of many species. Climate change will likely alter dormancy dynamics in zooplankton, the basis of aquatic food webs, by altering two important hatching cues: mean temperatures during the ice-free season, and mean day length when lakes become ice free. Theory suggests that these changes could alter diversity, hatchling abundances and phenology within lakes, and that these responses may diverge across latitudes due to differences in optimal hatching cues and strategies. 2.To examine the role of temperature and day length on hatching dynamics, we collected sediment from 25 lakes across a 1800 km latitudinal gradient and exposed sediment samples to a factorial combination of two photoperiods (12 and 16 hours) and two temperatures (8°C and 12 °C) representative of historical southern (short photoperiod, warm) and northern (long photoperiod, cool) lake conditions. We tested whether sensitivity to these hatching cues varies by latitudinal origin and differs among taxa. 3.Higher temperatures advanced phenology for all taxa, and these advances were greatest for cladocerans followed by copepods and rotifers. Although phenology differed among taxa, the effect of temperature did not vary with latitude. The latitudinal origin of the egg bank influenced egg abundance and hatchling abundance and diversity, with these latter effects varying with taxa, temperature and photoperiod. 4.Copepod hatchling abundances peaked at mid latitudes in the high temperature and long photoperiod treatments, whereas hatchling abundances of other zooplankton were greatest at low latitudes and high temperature. The overall diversity of crustacean zooplankton (copepods and cladocerans) also reflected distinct responses of each taxa to our treatments, with the greatest diversity occurring at mid latitudes ({\textasciitilde}56° N) in the shorter photoperiod treatment. 5.Our results demonstrate that hatching cues differ for broad taxonomic groups that vary in developmental and life-history strategies. These differences are predicted to drive latitude-specific shifts in zooplankton emergence with climate change, and could alter the base of aquatic food webs. This article is protected by copyright. All rights reserved.},
	language = {en},
	urldate = {2015-11-26},
	journal = {Journal of Animal Ecology},
	author = {Jones, Natalie T. and Gilbert, Benjamin},
	month = nov,
	year = {2015},
	keywords = {Daphnia, SH, eggs, latitude, Cladocera, egg production, Temperature, Copepods, diapause, freshwater, Rotifera, photoperiodism},
	pages = {n/a--n/a},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/S7RW3ZZU/Jones und Gilbert - 2015 - Changing climate cues differentially alter zooplan.pdf:application/pdf;Jones und Gilbert - 2015 - Changing climate cues differentially alter zooplan.pdf:/Users/fhoering/Zotero/storage/D9BNR2SA/Jones und Gilbert - 2015 - Changing climate cues differentially alter zooplan.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/GW3QF58B/abstract.html:text/html}
}

@article{chen_drosophila_2015,
	title = {Drosophila {Ionotropic} {Receptor} 25a mediates circadian clock resetting by temperature},
	volume = {advance online publication},
	copyright = {© 2015 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {0028-0836},
	url = {http://www.nature.com/nature/journal/vaop/ncurrent/full/nature16148.html},
	doi = {10.1038/nature16148},
	abstract = {Circadian clocks are endogenous timers adjusting behaviour and physiology with the solar day. Synchronized circadian clocks improve fitness and are crucial for our physical and mental well-being. Visual and non-visual photoreceptors are responsible for synchronizing circadian clocks to light, but clock-resetting is also achieved by alternating day and night temperatures with only 2–4 °C difference. This temperature sensitivity is remarkable considering that the circadian clock period ({\textasciitilde}24 h) is largely independent of surrounding ambient temperatures. Here we show that Drosophila Ionotropic Receptor 25a (IR25a) is required for behavioural synchronization to low-amplitude temperature cycles. This channel is expressed in sensory neurons of internal stretch receptors previously implicated in temperature synchronization of the circadian clock. IR25a is required for temperature-synchronized clock protein oscillations in subsets of central clock neurons. Extracellular leg nerve recordings reveal temperature- and IR25a-dependent sensory responses, and IR25a misexpression confers temperature-dependent firing of heterologous neurons. We propose that IR25a is part of an input pathway to the circadian clock that detects small temperature differences. This pathway operates in the absence of known ‘hot’ and ‘cold’ sensors in the Drosophila antenna, revealing the existence of novel periphery-to-brain temperature signalling channels.},
	language = {en},
	urldate = {2015-11-24},
	journal = {Nature},
	author = {Chen, Chenghao and Buhl, Edgar and Xu, Min and Croset, Vincent and Rees, Johanna S. and Lilley, Kathryn S. and Benton, Richard and Hodge, James J. L. and Stanewsky, Ralf},
	month = nov,
	year = {2015},
	keywords = {Entrainment, SH, Temperature, Drosophila, clock resetting},
	file = {Chen et al. - 2015 - Drosophila Ionotropic Receptor 25a mediates circad.pdf:/Users/fhoering/Zotero/storage/VEW27IV9/Chen et al. - 2015 - Drosophila Ionotropic Receptor 25a mediates circad.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/VPAH74WV/nature16148.html:text/html}
}

