@article{cuzin-roudy_seasonal_2000,
	title = {Seasonal reproduction, multiple spawning, and fecundity in northern krill, {Meganyctiphanes} norvegica , and {Antarctic} krill, {Euphausia} superba},
	volume = {57},
	issn = {0706-652X},
	url = {http://www.nrcresearchpress.com/doi/abs/10.1139/f00-165},
	doi = {10.1139/f00-165},
	abstract = {Both northern krill, Meganyctiphanes norvegica, and Antarctic krill, Euphausia superba, release eggs seasonally in multiple spawning events. Fecundity estimates were determined based on counts of mature oocytes in the ovary of females in preserved field samples. Principal components analysis and allometry were used to analyze the covariation of egg-batch size with female characteristics. In both krill species, egg-batch size scaled isometrically with ovarian weight but showed a positive allometry with body size. Predicted egg-batch size ranged from 200 to 4000 eggs for M. norvegica of 25.5-35 mm body length and from 1500 to 6000 eggs for E. superba of 36-55 mm. Allometric relationships can be used to predict annual fecundity from size structure data of krill populations. In this study, the effect of temperature on the duration of molting and spawning cycles was used to infer a number of reproductive cycles per year. taking into account climate and seasonal variation.},
	number = {S3},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Cuzin-Roudy, Janine},
	year = {2000},
	keywords = {history, spawning, fecundity, Krill, ovaries, vitellogenesis, maturity, size, molting, Growth, Prydz Bay, seasonal cycle, previtellogenesis, ovarian development, Crustacea},
	pages = {6--15},
	annote = {(0) Paper discussion on Tuesday, Feb 21st Despite krills importance, krill fecundity is still difficult to estimate. Brood size and reproductive periodicity are still debated. The authors goal was to establish realistic estimates of fecundity as a basis for numerical models to predict krill fecundity at population level based on anatomical/morphological characteristics of female krill. Results were disregarded for the most part (though we did have a fruitful debate about allometry a few days earlier...). Instead, our discussion focused on the seasonal pattern of ovarian development. (In future discussion, we will try and stick closer to the actual paper and the authors findings.)},
	file = {Cuzin Roudy 2000 (Spawning, repodiction etc. in northern and antactic krill).pdf:/Users/fhoering/Zotero/storage/RFRSBN2M/Cuzin Roudy 2000 (Spawning, repodiction etc. in northern and antactic krill).pdf:application/pdf;Cuzin-Roudy 2000 Seasonal reproduction, multiple s.pdf:/Users/fhoering/Zotero/storage/JUPV6ZQ5/Cuzin-Roudy 2000 Seasonal reproduction, multiple s.pdf:application/pdf}
}

