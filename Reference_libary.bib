	keywords = {biogeochemistry, marine biology, Biooceanography, Marine chemistry},
	pages = {219--221},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/3PPKZD4T/Duprat et al. - 2016 - Enhanced Southern Ocean marine productivity due to.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/Q9K2WR7J/ngeo2633.html:text/html}
}

@article{jaccard_covariation_2016,
	title = {Covariation of deep {Southern} {Ocean} oxygenation and atmospheric {CO}2 through the last ice age},
