	pages = {1460--1471},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/QMP8ZS4A/Meyer et al. - 2015 - Pyrosequencing and de novo assembly of Antarctic k.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/ZEMNMA7D/abstract.html:text/html}
