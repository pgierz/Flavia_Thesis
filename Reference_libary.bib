	keywords = {Arctic fjords, Advection, SH, Arctic, Kongsfjorden, Water masses},
	pages = {133--166},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/TV3D6FN3/Svendsen et al. - 2002 - The physical environment of Kongsfjorden–Krossfjor.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/UNACS6VD/abstract.html:text/html}
}

