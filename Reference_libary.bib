	keywords = {spawning, fish, SH, Clock gene, Cryptochrome, Circadian clock, Lunar cycle, Moonlight, cry, Spawning, RNA Extraction, Circadian Rhythm, Phylogeny, Midbrain, Ovaries},
	pages = {e28643},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/2GCF95FA/Fukushiro et al. - 2011 - Lunar Phase-Dependent Expression of Cryptochrome a.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/U65P58RS/article.html:text/html}
