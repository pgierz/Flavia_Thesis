	pages = {127--142},
	file = {Conover - 1988 - Comparative life histories in the genera Calanus a.pdf:/Users/fhoering/Zotero/storage/5WDW4ZKW/Conover - 1988 - Comparative life histories in the genera Calanus a.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/HQV2WB7E/10.html:text/html}
