	pages = {95--106},
	file = {Aquatic Microbial Ecology Full Text PDF:/Users/fhoering/Zotero/storage/N3UR5VR6/Fiala et al. - 2006 - Spatial and seasonal heterogeneity of sea ice micr.pdf:application/pdf}
}

@article{goto_roles_2013,
