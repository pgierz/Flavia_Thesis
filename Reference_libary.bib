@article{marcus_variation_1984,
	title = {Variation in the diapause response of \textit{{Labidocera} aestiva} (copepoda: calanoida) from different latitudes and its importance in the evolutionary process},
	volume = {166},
	issn = {0006-3185},
	shorttitle = {Variation in the diapause response of labidocera aestiva (copepoda},
