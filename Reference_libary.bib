@article{pivarciova_unexpected_2016,
	title = {Unexpected {Geographic} {Variability} of the {Free} {Running} {Period} in the {Linden} {Bug} \textit{{Pyrrhocoris} apterus}},
	issn = {0748-7304, 1552-4531},
	url = {http://jbr.sagepub.com/content/early/2016/10/04/0748730416671213},
	doi = {10.1177/0748730416671213},
	abstract = {Circadian clocks keep organisms in synchrony with external day-night cycles. The free running period (FRP) of the clock, however, is usually only close to—not exactly—24 h. Here, we explored the geographical variation in the FRP of the linden bug, Pyrrhocoris apterus, in 59 field-lines originating from a wide variety of localities representing geographically different environments. We have identified a remarkable range in the FRPs between field-lines, with the fastest clock at {\textasciitilde}21 h and the slowest close to 28 h, a range comparable to the collections of clock mutants in model organisms. Similarly, field-lines differed in the percentage of rhythmic individuals, with a minimum of 13.8\% and a maximum of 86.8\%. Although the FRP correlates with the latitude and perhaps with the altitude of the locality, the actual function of this FRP diversity is currently unclear. With the recent technological progress of massive parallel sequencing and genome editing, we can expect remarkable progress in elucidating the genetic basis of similar geographic variants in P. apterus or in similar emerging model species of chronobiology.},
	language = {en},
	urldate = {2016-10-12},
	journal = {Journal of Biological Rhythms},
	author = {Pivarciova, Lenka and Vaneckova, Hanka and Provaznik, Jan and Wu, Bulah Chia-hsiang and Pivarci, Martin and Peckova, Olga and Bazalova, Olga and Cada, Stepan and Kment, Petr and Kotwica-Rolinska, Joanna and Dolezel, David},
	month = oct,
	year = {2016},
	pmid = {27708112},
	keywords = {SH, locomotor activity, latitude, Diapause, Circadian clock, Circadian Rhythm, Insects, phylogeography, Pyrrhocoris apterus, altitude, population rhythm, geographic distribution},
	pages = {0748730416671213},
	file = {Pivarciova et al. - 2016 - Unexpected Geographic Variability of the Free Runn.pdf:/Users/fhoering/Zotero/storage/7CSQFZWG/Pivarciova et al. - 2016 - Unexpected Geographic Variability of the Free Runn.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/8VKHCZ3I/0748730416671213.html:text/html}
