	file = {Springer Full Text PDF:/Users/fhoering/Zotero/storage/7596HXNK/Lang-Ouellette et al. - 2014 - Mammalian hibernation and regulation of lipid meta.pdf:application/pdf}
}

@article{stevenson_reversible_2013,
	title = {Reversible {DNA} methylation regulates seasonal photoperiodic time measurement},
