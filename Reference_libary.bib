	abstract = {Cuticle tanning (or sclerotization and pigmentation) in invertebrates involves the oxidative conjugation of proteins, which renders them insoluble and hardens and darkens the color of the exoskeleton. Two kinds of phenoloxidases, laccase and tyrosinase, have been proposed to participate in tanning, but proof of the true identity of the enzyme(s) responsible for this process has been elusive. We report the cloning of cDNAs for laccases and tyrosinases from the red flour beetle, Tribolium castaneum, as well as their developmental patterns of expression. To test for the involvement of these types of enzymes in cuticle tanning, we performed RNA interference experiments to decrease the levels of individual phenoloxidases. Normal phenotypes were obtained after dsRNA-mediated transcript depletion for all phenoloxidases tested, with the exception of laccase 2. Insects injected with dsRNA for the laccase 2 gene failed to tan, were soft-bodied and deformed, and subsequently died in a dsRNA dose-dependent fashion. The results presented here support the hypothesis that two isoforms of laccase 2 generated by alternative splicing catalyze larval, pupal, and adult cuticle tanning in Tribolium.},
	language = {en},
	number = {32},
	urldate = {2018-10-02},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Arakane, Yasuyuki and Muthukrishnan, Subbaratnam and Beeman, Richard W. and Kanost, Michael R. and Kramer, Karl J.},
	month = aug,
	year = {2005},
	pmid = {16076951},
	keywords = {pigmentation, RNA interference, exoskeleton, sclerotization, Tribolium castaneum},
	pages = {11337--11342},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/J7J5RR99/Arakane et al. - 2005 - Laccase 2 is the phenoloxidase gene required for b.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/YH2V5UNF/11337.html:text/html}
}

@article{song_kruppel-homolog_2014,
	title = {Krüppel-homolog 1 mediates juvenile hormone action to promote vitellogenesis and oocyte maturation in the migratory locust},
	volume = {52},
	issn = {0965-1748},
	url = {http://www.sciencedirect.com/science/article/pii/S0965174814001131},
	doi = {10.1016/j.ibmb.2014.07.001},
	abstract = {Juvenile hormone (JH) prevents insect larval metamorphosis and stimulates processes for adult reproduction. Krüppel-homolog 1 (Kr-h1), a zinc finger transcription factor, is shown to mediate the anti-metamorphic effect of JH in both holometabolous and hemimetabolous insects. However, the role of Kr-h1 in JH-mediated reproduction has not been determined. Using the migratory locust, Locusta migratoria, we showed here that Kr-h1 was expressed in response to JH in female adults, and Kr-h1 transcription was directly regulated by the JH-receptor complex comprised of Methoprene-tolerant (Met) and steroid receptor co-activator. We demonstrated that Kr-h1 RNAi phenocopied Met RNAi and JH-deprived condition during post-eclosion development and vitellogenesis of female locusts. Knockdown of Kr-h1 resulted in substantial reduction of Vg expression in the fat body and lipid accumulation in the primary oocytes, accompanied by blocked follicular epithelium development, oocyte maturation and ovarian growth. Our data therefore reveal a crucial role of Kr-h1 in insect vitellogenesis and egg production. This study suggests that JH-Met-Kr-h1 signaling pathway is also functional in insect reproduction.},
	urldate = {2018-10-02},
	journal = {Insect Biochemistry and Molecular Biology},
	author = {Song, Jiasheng and Wu, Zhongxia and Wang, Zhiming and Deng, Shun and Zhou, Shutang},
	month = sep,
	year = {2014},
	keywords = {Methoprene-tolerant, Juvenile hormone, Kr-h1, Lipid accumulation, Oocyte maturation, Vitellogenesis},
	pages = {94--101},
	file = {ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/EL3G9SS9/S0965174814001131.html:text/html;Song et al. - 2014 - Krüppel-homolog 1 mediates juvenile hormone action.pdf:/Users/fhoering/Zotero/storage/4FCCGICM/Song et al. - 2014 - Krüppel-homolog 1 mediates juvenile hormone action.pdf:application/pdf}
}

