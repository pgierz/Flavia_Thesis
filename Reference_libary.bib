	month = feb,
	year = {2011},
	keywords = {baseline data, ecological footprints, Arctic Ocean, climate change, marine ecosystems},
	pages = {1235--1249},
	file = {Snapshot:/Users/fhoering/Zotero/storage/T8R5QUI2/abstract.html:text/html}
}

