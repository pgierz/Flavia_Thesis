	pages = {11--32},
	file = {Marine Ecology Progress Series Full Text PDF:/Users/fhoering/Zotero/storage/B5GU77WZ/Ross et al. - 2014 - Trends, cycles, interannual variability for three .pdf:application/pdf;Marine Ecology Progress Series Snapshot:/Users/fhoering/Zotero/storage/F6TJJHXS/p11-32.html:text/html}
}

