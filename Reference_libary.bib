	keywords = {Molluscs, SH, Circadian Rhythm, bivalve, circatidal clock, marine clocks, oyster, Crassostrea gigas, circatidal rhythm},
	pages = {89--99},
	file = {Mat et al. - 2014 - Looking for the clock mechanism responsible for ci.pdf:/Users/fhoering/Zotero/storage/KAHA9FIX/Mat et al. - 2014 - Looking for the clock mechanism responsible for ci.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/CU76KW3A/s00227-013-2317-2.html:text/html}
}

@article{greeley_annual_1983,
