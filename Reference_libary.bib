@article{lewis_damped_1987,
	title = {A damped circadian oscillator model of an insect photoperiodic clock. {I}. {Description} of the model based on a feedback control system},
	volume = {128},
	issn = {0022-5193},
	url = {http://www.sciencedirect.com/science/article/pii/S0022519387800309},
	doi = {10.1016/S0022-5193(87)80030-9},
	abstract = {We describe a simple model for photoperiodic time measurement in the flesh fly Sarcophaga argyrostoma, based on a damped circadian oscillator and the principles of “external coincidence”. The damped oscillator is derived from a feedback control systems approach to circadian rhythmicity. Adoption of a damped oscillator allows us to simulate the complex shape of the photoperiodic response curve, particularly the fall in diapause occurring in very short daylengths and in darkness. Slight alterations in some of the components of the model allow simulations of time measurement in other insects.},
	number = {1},
