@article{wickham_sa_krill_2005,
	title = {Krill larvae, copepods and the microbial food web: interactions during the {Antarctic} fall},
	abstract = {Experiments conducted in the Bellinghausen Sea (Antarctica) during the austral autumn evaluated the interactive effects of krill larvae (furcilia) and copepods on the microbial food web. Although krill and their larvae are primarily considered to be herbivores, the experiments showed that furcilia have much higher grazing rates on copepods and ciliates than on algae. Despite the large predation impact of furcilia on ciliates, the trophic levels beneath ciliates did not respond to the addition of furcilia, suggesting that top-down control of the Antarctic microbial food web does not play a major role during the austral autumn. The experiments also demonstrate that predation on heterotrophic food organisms is a plausible explanation for the survival of furcilia over the Antarctic winter.},
