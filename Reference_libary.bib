@article{taguchi_effect_1997,
	series = {Canada-{Japan} {SARES} {Project} on {First}-{Year} {Sea} {Ice}},
	title = {Effect of silicate enrichment on ice algae at low salinity in {Saroma}-ko {Lagoon}, {Hokkaido}, {Japan}},
