	keywords = {gene expression, housekeeper, qPCR, SH, HSP, Temperature, Heat shock proteins, EF1, photoperiodism},
	pages = {e53006},
	file = {Li et al 2013_SH.pdf:/Users/fhoering/Zotero/storage/MVU863PD/Li et al 2013_SH.pdf:application/pdf;Li et al. - 2013 - Reference Gene Selection for qRT-PCR Analysis in t.pdf:/Users/fhoering/Zotero/storage/F5H7ZBK6/Li et al. - 2013 - Reference Gene Selection for qRT-PCR Analysis in t.pdf:application/pdf}
