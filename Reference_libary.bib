@article{van_haren_diel_2013,
	title = {Diel {Vertical} {Migration} in {Deep} {Sea} {Plankton} {Is} {Finely} {Tuned} to {Latitudinal} and {Seasonal} {Day} {Length}},
	volume = {8},
	url = {http://dx.doi.org/10.1371/journal.pone.0064435},
	doi = {10.1371/journal.pone.0064435},
	abstract = {Diel vertical migration (DVM) is a ubiquitous phenomenon in marine and freshwater plankton communities. Most commonly, plankton migrate to surface waters at dusk and return to deeper waters at dawn. Up until recently, it was thought that DVM was triggered by a relative change in visible light intensity. However, evidence has shown that DVM also occurs in the deep sea where no direct and background sunlight penetrates. To identify whether such DVM is associated with latitudinal and seasonal day light variation, one and a half years of recorded acoustic data, a measure of zooplankton abundance and movement, were examined. Acoustic Doppler current profilers, moored at eight different sub-tropical latitudes in the North-Atlantic Ocean, measured in the vertical range of 500–1600 m. DVM was observed to follow day length variation with a change in season and latitude at all depths. DVM followed the rhythm of local sunrise and sunset precisely between 500 and 650 m. It continued below 650 m, where the deepest penetrable irradiance level are {\textless}10−7 times their near-surface values, but plankton shortened their time at depth by up to about 63\% at 1600 m. This suggests light was no longer a cue for DVM. This trend stayed consistent both across latitudes and between the different seasons. It is hypothesized that another mechanism, rather than light, viz. a precise biochemical clock could maintain the solar diurnal and seasonal rhythms in deep sea plankton motions. In accordance with this hypothesis, the deepest plankton were consistently the first to migrate upwards.},
	number = {5},
	urldate = {2014-10-22},
	journal = {PLoS ONE},
	author = {van Haren, Hans and Compton, Tanya J.},
	month = may,
	year = {2013},
	keywords = {deep sea, SH, Light, latitude, DVM, seasonal cycle, Circadian Rhythm, zooplankton, photoperiodism},
	pages = {e64435},
	file = {van Haren und Compton - 2013 - Diel Vertical Migration in Deep Sea Plankton Is Fi.pdf:/Users/fhoering/Zotero/storage/BWVWHE8M/van Haren und Compton - 2013 - Diel Vertical Migration in Deep Sea Plankton Is Fi.pdf:application/pdf}
}

