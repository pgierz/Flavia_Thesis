	journal = {Journal of Geophysical Research: Oceans},
	author = {Muench, R. D. and Morison, J. H. and Padman, L. and Martinson, D. and Schlosser, P. and Huber, B. and Hohmann, Roland},
	month = feb,
	year = {2001},
	keywords = {4203 Oceanography: General: Analytical modeling and laboratory experiments, 4200 Oceanography: General},
	pages = {2423--2440},
	file = {Muenich et al. 2001 (Maud Rise).pdf:/Users/fhoering/Zotero/storage/A8XBBZF9/Muenich et al. 2001 (Maud Rise).pdf:application/pdf}
}

