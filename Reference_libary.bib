	keywords = {metabolic reduction, Anoxia, Temperature adaptation, Caenorhabditis elegans, NADH fluorescence, Enzymes, enzyme activity, Adaptation, Redox state},
	pages = {469--479},
	file = {Paul et al. 2000 (metabolic adaptations, key enzymes).pdf:/Users/fhoering/Zotero/storage/65RIDGS3/Paul et al. 2000 (metabolic adaptations, key enzymes).pdf:application/pdf}
}

