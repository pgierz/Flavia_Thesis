	keywords = {Ecology, Microbiology, Oceanography, Plant Sciences, Waste Water Technology / Water Pollution Control / Water Management / Aquatic Pollution, Zoology},
	pages = {107--116},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/XAWN34GU/Anger et al. - 1985 - Effects of starvation on moult cycle and hepatopan.pdf:application/pdf}
}

@article{ross_effect_1988,
