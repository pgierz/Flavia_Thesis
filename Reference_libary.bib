	author = {Gutt, Julian and Bertler, Nancy and Bracegirdle, Thomas J. and Buschmann, Alexander and Comiso, Josefino and Hosie, Graham and Isla, Enrique and Schloss, Irene R. and Smith, Craig R. and Tournadre, Jean and Xavier, José C.},
	month = apr,
	year = {2015},
	keywords = {ozone depletion, sea-ice, warming, Icebergs, acidification, habitats, ice-shelves},
	pages = {1434--1453},
	file = {Full Text PDF:/Users/fhoering/Zotero/storage/Z37QFBT8/Gutt et al. - 2015 - The Southern Ocean ecosystem under multiple climat.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/KUF6TSHA/full.html:text/html}
