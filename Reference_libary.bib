	pages = {21--47},
	annote = {(0) 10.1007/978-3-7091-0973-1\_2},
	file = {Hansen 2012 Paleoclimate Implications for Human-Ma.pdf:/Users/fhoering/Zotero/storage/2U96K7F7/Hansen 2012 Paleoclimate Implications for Human-Ma.pdf:application/pdf}
}

