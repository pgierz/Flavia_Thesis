	keywords = {per, Photoreception, mutants, Entrainment, SH, locomotor activity, disconnected, Period, phase shifts, limits of entrainment, arrhythmic, Drosophila, sine oculis},
	pages = {67--94},
	file = {Snapshot:/Users/fhoering/Zotero/storage/S96M5EV9/67.html:text/html}
