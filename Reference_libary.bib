@article{wood_introduction_1973,
	title = {An {Introduction} to the {Phytoplankton}, {Primary} {Production} and {Relevant} {Hydrography} of {Loch} {Etive}},
	volume = {61},
	issn = {0022-0477},
	url = {http://www.jstor.org/stable/2259045},
	doi = {10.2307/2259045},
	abstract = {(1) This paper describes some of the results of an initial study in 1970/71 of the phytoplankton primary production of the lower basin of Loch Etive, a sea-loch on the west coast of Scotland. (2) Etive is separated from the sea by a narrow entrance and sill at Connel. It is about 30 km long and is further divided into two basins at Bonawe. The lower basin contains an additional sill. The loch has a large catchment area, and the resulting freshwater inflow results in a reduced average salinity in the loch. Current studies show that the loch can be analysed in terms of a two-layer system, mean separation of surface (brackish, seawards flowing) and deep (salty, landwards flowing) layers proceeding at 6-8 km day-1. (3) The upper 10 m of the water column correspond approximately to both the brackish layer and the euphotic zone. Phytoplankton standing crop in this zone of the lower loch ranged from about 7 to about 250 mg C m-3 based on pigment measurements. Cell volume measurements resulted in lower estimates. A high level of standing crop was reached in March and persisted until July/August. (4) Skeletonema costatum was the dominant phytoplankter. Small flagellates were also important. (5) About 70 g C m-2 year-1 is a rough estimate of gross annual primary production in the euphotic zone of the lower basin. A number of factors qualify this value, most important being difficulties in measuring CO2 concentrations, and day-to-day fluctuations in light intensity. (6) It is considered that light is the most important limiting factor in the loch, and that the effects of the wet west highland climate dominate the ecology of the phytoplankton. (7) Comparisons are made with published studies of Loch Ewe, Sween and Striven. The last appears most similar to Etive.},
	number = {2},
	urldate = {2016-11-18},
	journal = {Journal of Ecology},
	author = {Wood, B. J. B. and Tett, P. B. and Edwards, A.},
	year = {1973},
	keywords = {Loch Etive, Chlorophyll a, SH, Scotland, Carbon, Phytoplankton, carbon content},
	pages = {569--585},
	file = {Wood et al. - 1973 - An Introduction to the Phytoplankton, Primary Prod.pdf:/Users/fhoering/Zotero/storage/KPPTDQKQ/Wood et al. - 1973 - An Introduction to the Phytoplankton, Primary Prod.pdf:application/pdf}
}

@article{freese_year-round_2017,
	title = {A year-round study on metabolic enzymes and body composition of the {Arctic} copepod \textit{{Calanus} glacialis}: implications for the timing and intensity of diapause},
	volume = {164},
	issn = {0025-3162, 1432-1793},
	shorttitle = {A year-round study on metabolic enzymes and body composition of the {Arctic} copepod {Calanus} glacialis},
	url = {http://link.springer.com/article/10.1007/s00227-016-3036-2},
	doi = {10.1007/s00227-016-3036-2},
	abstract = {Knowledge on the capability of zooplankton to adapt to the rapidly changing environmental conditions in the Arctic is crucial to predict future ecosystem processes. The key species on the Arctic shelf, the calanoid copepod Calanus glacialis, grows and accumulates lipid reserves in spring and summer in surface waters. The winter is spent in dormancy in deeper water layers with low metabolic activity. As timing and intensity of metabolic changes have been poorly investigated, our study aims to characterize the physiology of C. glacialis over an entire year, from July 2012 to July 2013. We followed anabolic and catabolic enzyme activities and the biochemical composition of this species, taking depth-stratified samples once a month in Billefjorden, a high-Arctic sill fjord. A large part of the population had migrated to depths {\textgreater}100 m by July 2012. Only thereafter, anabolic activities decreased slowly, suggesting that low metabolism is related to ceased feeding rather than to endogenous regulation. During overwintering, anabolic enzyme activities were reduced by half as compared to peak activities in spring. The biochemical composition of the copepods changed little from July to December. Then, the lipid catabolic activity increased and the lipid content decreased, likely fuelling moulting and gonad maturation. The protein content did not change significantly during winter, suggesting that proteins are not much catabolized during that time. The relatively high metabolic activity in C. glacialis in winter suggests that this species is not entering a true diapause and should thus be able to respond flexible to changing environmental conditions.},
	language = {en},
	number = {1},
	urldate = {2016-12-05},
	journal = {Marine Biology},
	author = {Freese, Daniela and Søreide, Janne E. and Graeve, Martin and Niehoff, Barbara},
	month = jan,
	year = {2017},
	keywords = {HOAD, Lipid, SH, Arctic, Calanus glacialis, MDH, Billefjorden, Enzymes, Cgla, seasonal cycle, Diapause, early/late, enzyme activity, CS, lipid content, Calanoid copepods, Calanus, Copepods, diapause, Svalbard},
	pages = {3},
	file = {Freese et al. - 2017 - A year-round study on metabolic enzymes and body c.pdf:/Users/fhoering/Zotero/storage/MZK52FDF/Freese et al. - 2017 - A year-round study on metabolic enzymes and body c.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/6WGAWAQ7/s00227-016-3036-2.html:text/html}
}

@article{jager_modelling_2017,
	title = {Modelling the dynamics of growth, development and lipid storage in the marine copepod \textit{{Calanus} finmarchicus}},
	volume = {164},
	issn = {0025-3162, 1432-1793},
	url = {http://link.springer.com/article/10.1007/s00227-016-3030-8},
	doi = {10.1007/s00227-016-3030-8},
	abstract = {Mechanistic models are essential tools for interpreting and predicting the consequences of a changing environment and stressors such as pollution on the life histories of marine organisms. Here, we apply the simple and generic energy-budget model DEBkiss to the life history of the marine copepod Calanus finmarchicus. Model modifications were needed to accommodate the copepod life cycle, which deviates in several respects from most other animals (e.g., a sudden stop of growth after the final moult). We identified an acceleration of growth in the early copepodite stages, which could be linked to an increase in the specific feeding rate of the animals. Lipid storage, an essential element of C. finmarchicus biology, was successfully captured with the reproduction buffer of the DEBkiss model. The resulting model was fitted to a detailed data set from the literature and was able to explain growth, development and lipid storage from egg to adult, at different temperatures and food availabilities, within a single consistent framework. The parameterised model could subsequently be used to elucidate the energetic constraints on gonad maturation and reproduction. Interestingly, the overhead costs for egg production seem to be substantially higher than the default value applied in DEB-based studies. The current model provides a solid basis for applications in stress ecology, although our model analysis also identified several knowledge gaps. Specifically, further research is needed to cover the dynamics of diapause and gonad maturation, to explain the dependence of maximum body size on food and temperature, and to verify the predicted high costs for maturity maintenance.},
	language = {en},
	number = {1},
	urldate = {2016-12-05},
	journal = {Marine Biology},
	author = {Jager, Tjalling and Salaberria, Iurgi and Altin, Dag and Nordtug, Trond and Hansen, Bjørn Henrik},
	month = jan,
	year = {2017},
	keywords = {Cfin, Lipid, SH, Development, Growth, development time, model, lipid content, Calanoid copepods, Calanus, Calanus finmarchicus, Copepods},
	pages = {1},
	file = {Jager et al. - 2017 - Modelling the dynamics of growth, development and .pdf:/Users/fhoering/Zotero/storage/4NP3DGRD/Jager et al. - 2017 - Modelling the dynamics of growth, development and .pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/4JWEEB84/s00227-016-3030-8.html:text/html}
}

@article{zhu_differential_2017,
	title = {Differential expression of circadian clock genes in two strains of beetles reveals candidates related to photoperiodic induction of summer diapause},
	issn = {0378-1119},
	url = {http://www.sciencedirect.com/science/article/pii/S0378111916309623},
	doi = {10.1016/j.gene.2016.12.004},
	abstract = {Diapause (also known as dormancy) is a state of arrested development induced by photoperiod or temperature that allows insects to survive adverse environmental conditions. By regulating diapause induction, the circadian clock is involved in short-day-induced winter diapause but whether this is also the case in long-day (LD)-induced summer diapause remains unknown. The cabbage beetle Colaphellus bowringi could enter summer diapause under LD conditions. However, a non-photoperiodic-diapause (NPD) strain of this species, which was developed in our laboratory by artificial selection, could not enter diapause under LD photoperiod. Therefore, we identified circadian clock genes in this species and measured differences in their expression between a high diapause (HD) strain and the NPD strain to investigate the potential relationship between circadian clock genes and summer diapause induction in C. bowringi. We successfully cloned eight circadian clock genes and obtained intact ORFs of four; cryptochrome2, double-time, shaggy and vrille. Phylogenetic trees and sequence alignment analyses indicated that these circadian clock genes were conserved across insect taxa. The quantitative real-time PCR indicated that clock, cycle, period, timeless, cryptochrome2, and vrille were differentially expressed between HD and NPD strains reared under LD photoperiod during the diapause induction phase. These findings suggest the potential relationship between circadian clock genes and LD-regulated summer diapause induction in C. bowringi.},
	urldate = {2016-12-13},
	journal = {Gene},
	author = {Zhu, Li and Liu, Wen and Tan, Qian-Qian and Lei, Chao-Liang and Wang, Xiao-Ping},
	year = {2017},
	keywords = {SH, Clock gene, Circadian clock, Insects, diapause, photoperiodic time measurement, Colaphellus bowringi, photoperiodism},
	file = {ScienceDirect Snapshot:/Users/fhoering/Zotero/storage/DXC7DAVB/S0378111916309623.html:text/html;Zhu et al. - 2017 - Differential expression of circadian clock genes i.pdf:/Users/fhoering/Zotero/storage/RE3RGFU2/Zhu et al. - 2017 - Differential expression of circadian clock genes i.pdf:application/pdf}
}

