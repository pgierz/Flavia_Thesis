@article{laconte_structural_2013,
	title = {Structural constraints and functional divergences in {CASK} evolution},
	volume = {41},
	issn = {1470-8752},
	doi = {10.1042/BST20130061},
	abstract = {CASK (Ca2+/calmodulin-activated serine kinase) is a synaptic protein that interacts with the cytosolic tail of adhesion molecules such as neurexins, syncam and syndecans. It belongs to the MAGUK (membrane-associated guanylate kinase) family of scaffolding proteins which are known to decorate cell-cell junctions. CASK is an essential gene in mammals, critical for neurodevelopment. Mutations in the CASK gene in humans result in phenotypes that range from intellectual disability to lethality. Despite its importance, CASK has a single genetic isoform located in the short arm of the X chromosome near an evolutionary breakpoint. Surprisingly, CASK is a non-essential gene in invertebrates and displays functional divergence. In the present article, we describe the phylogenetic differences in existing CASK orthologues. The CASK gene has undergone a huge expansion in size ({\textasciitilde}55-fold). Almost all of this expansion is a direct result of an increase in the size of the introns. The coding region of CASK orthologues, and hence the protein, exhibit a high degree of evolutionary conservation. Within the protein, domain arrangement is completely conserved and substitution rates are higher in the connecting loop regions [L27 (Lin2, Lin7)] than within the domain. Our analyses of single residue substitutions and genotype-phenotype relationships suggest that, other than intronic expansion, the dramatic functional changes of CASK are driven by subtle (non-radical) primary structure changes within the CASK protein and concomitant changes in its protein interactors.},
	language = {eng},
	number = {4},
	journal = {Biochemical Society Transactions},
	author = {LaConte, Leslie and Mukherjee, Konark},
	month = aug,
	year = {2013},
	pmid = {23863172},
	keywords = {Protein Conformation, Animals, Humans, Phylogeny, Chromosomes, Human, X, Evolution, Molecular, Guanylate Kinase, Introns, Models, Molecular},
	pages = {1017--1022},
	file = {Caskevolution.pdf:/Users/fhoering/Zotero/storage/2NCP9C72/Caskevolution.pdf:application/pdf}
}

