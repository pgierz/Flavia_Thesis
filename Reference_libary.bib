	pages = {1263--1278},
	file = {Fortier et al. - 2001 - Visual predators and the diel vertical migration o.pdf:/Users/fhoering/Zotero/storage/QPE7CIBJ/Fortier et al. - 2001 - Visual predators and the diel vertical migration o.pdf:application/pdf;Snapshot:/Users/fhoering/Zotero/storage/2MXX2MIU/1263.html:text/html}
